{
  "compilerOptions": {
    "jsx": "react",
    "target": "esnext",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*"]
}
